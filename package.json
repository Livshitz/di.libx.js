{
  "name": "di.libx.js",
  "version": "0.1.8",
  "main": "./src/index",
  "types": "./build",
  "license": "MIT",
  "author": "",
  "scripts": {
    "build": "tsc",
    "build-browser": "pax-browserify src/browser.ts dist -y --minify",
    "watch": "tsc -w",
    "main": "node build/Main.js",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage --coverageReporters=cobertura --coverageReporters=html",
    "bump": "./bump.sh"
  },
  "files": [
    "build/**",
    "dist/**",
    "src/**",
    "*.js",
    "*.ts"
  ],
  "dependencies": {
    "concurrency.libx.js": "^0.2.23",
    "typescript": "^3.9.3"
  },
  "devDependencies": {
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "jest": "^25.1.0",
    "jest-junit": "^10.0.0",
    "jest-junit-reporter": "^1.1.0",
    "pax.libx.js": "^0.3.4",
    "ts-jest": "^25.2.0"
  },
  "jest": {
    "coverageReporters": [
      "cobertura",
      "html"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "outputDirectory": "build/test_reports",
    "output": "./.tmp/test/TEST-RESULTS.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  }
}
